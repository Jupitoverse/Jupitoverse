  "classification_definitions": {
    "Near-Earth Asteroid (NEA)": {
      "definition": "Asteroid with perihelion distance < 1.3 AU",
      "subclasses": {
        "Atiras": "Semi-major axis < 1.0 AU and aphelion < 0.983 AU",
        "Atens": "Semi-major axis < 1.0 AU and aphelion > 0.983 AU",
        "Apollos": "Semi-major axis > 1.0 AU and perihelion < 1.017 AU",
        "Amors": "Semi-major axis > 1.0 AU and perihelion between 1.017 and 1.3 AU"
      }
    },
    "Potentially Hazardous Asteroid (PHA)": {
      "definition": "NEA with MOID < 0.05 AU and absolute magnitude H < 22.0",
      "criteria": [
        "Minimum Orbit Intersection Distance (MOID) with Earth < 0.05 AU (7.5 million km)",
        "Absolute magnitude H < 22.0 (diameter roughly > 140 meters)"
      ]
    },
    "Main Belt Asteroid": {
      "definition": "Asteroid orbiting between Mars and Jupiter",
      "semi_major_axis_range": "2.0 - 3.2 AU"
    },
    "Comet Types": {
      "Short-period": "Orbital period < 200 years",
      "Long-period": "Orbital period > 200 years",
      "Jupiter-family": "Orbital period < 20 years, Tisserand parameter > 2"
    }
  },
